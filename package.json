{
  "name": "makers-den-homepage",
  "homepage": "http://makersden.io/",
  "twitter": "makers_den",
  "repository": "https://github.com/makersden/homepage",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "start": "webpack-dashboard -- phenomic start",
    "build": "phenomic build",
    "pretest": "npm run lint",
    "test": "npm run build"
  },
  "phenomic": {
    "CNAME": true,
    "destination": "docs"
  },
  "#babel": "webpack-(development|production) are useful for webpack 2, otherwise use development|production",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "babel-preset-react-optimize",
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      },
      "webpack-development": {
        "presets": [
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "webpack-production": {
        "presets": [
          "babel-preset-react-optimize",
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": "./node_modules/phenomic/lib/eslint-config-recommended/index.js"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx-self": "^6.11.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.13.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "devicon": "^2.0.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^2.0.0",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.2",
    "node-sass": "^4.3.0",
    "npm-run-all": "^1.7.0",
    "phenomic": "^0.19.0",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-current-selector": "^0.0.3",
    "postcss-for": "^2.1.1",
    "postcss-loader": "^0.13.0",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-random": "^1.0.9",
    "postcss-reporter": "^1.3.0",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-custom-scrollbars": "^4.0.1",
    "react-dom": "^15.0.0-rc.1",
    "react-helmet": "^3.0.0",
    "react-hot-loader": "^3.0.0-beta",
    "react-redux": "^4.0.0",
    "react-router": "^2.3.0",
    "react-slick": "^0.14.5",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "redux": "^3.0.0",
    "sass-loader": "^4.1.1",
    "slick-carousel": "^1.6.0",
    "smooch": "^3.9.7",
    "style-loader": "^0.13.0",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.11.0",
    "webpack-dashboard": "^0.2.0",
    "whatwg-fetch": "^0.11.0"
  },
  "dependencies": {
    "react-disqus-comments": "^1.0.3",
    "react-ga": "^2.1.2",
    "smoothscroll": "^0.3.0"
  }
}
